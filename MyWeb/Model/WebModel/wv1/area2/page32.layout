<wr-page>
  <wr-content for="page-content">
    <wd-card title="main/dtl8">
      <wr-content for="content">
        <wd-container elements="main/dtl8/data/{Attribute}*">
          <wr-content for="element[_application_$appID]">
            <span>
              <span class="header">
                {{ labelOf(&#039;.&#039;) | wrLocalize }}
              </span>
              <span class="value">
                {{ valueOf(&#039;.&#039;) }}
              </span>
            </span>
          </wr-content>
          <wr-content for="element[_application_$name]">
