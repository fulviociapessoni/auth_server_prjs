<wr-page>
  <wr-content for="content">
    <imd-data-table cols="main/lst13/data/{Attribute}*" [rows]="valueOf('main/lst13/data')" events="main/lst13/data/{Event}*">
      <wr-content for="header[_application_$appID]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
        <imd-button appearance="clear" event="sortBy" iconPosition="icon-only" icon="chevron-down" size="small"></imd-button>
      </wr-content>
      <wr-content for="header[_application_$name]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="header[_application_$type]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="header[_application_$description]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="header[_application_$authorities]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="header[_application_$accessTokenValidity]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="header[_application_$refreshTokenValidity]">
        {{ labelOf(&#039;.&#039;) | wrLocalize }}
      </wr-content>
      <wr-content for="data[_application_$appID]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$name]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$type]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$description]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$authorities]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$accessTokenValidity]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="data[_application_$refreshTokenValidity]">
        {{ valueOf(&#039;.&#039;, row) }}
      </wr-content>
      <wr-content for="paginator">
        <imd-paginator></imd-paginator>
      </wr-content>
      <wr-content for="event[oslte41]">
        <imd-button></imd-button>
      </wr-content>
      <wr-content for="event[oslte42]">
        <imd-button></imd-button>
      </wr-content>
      <wr-content for="event[oslte43]">
        <imd-button></imd-button>
      </wr-content>
    </imd-data-table>
    <br>
    <br>
    <br>
    <div style="border: blue 2px solid">
      <imd-input context="main/frm18/fld63"></imd-input>
      <imd-input context="main/frm18/fld64"></imd-input>
      <imd-select context="main/frm18/sfld18"></imd-select>
      <imd-input context="main/frm18/fld66"></imd-input>
      <imd-input context="main/frm18/fld67"></imd-input>
      <imd-checkboxes context="main/frm18/sfld15"></imd-checkboxes>
      <imd-checkboxes context="main/frm18/sfld17"></imd-checkboxes>
      <imd-checkboxes context="main/frm18/sfld16"></imd-checkboxes>
      <imd-input context="main/frm18/fld68"></imd-input>
      <imd-input context="main/frm18/fld69"></imd-input>
      <br>
      <br>
      <imd-button context="main/frm18/oslte44"></imd-button>
      <imd-button context="main/frm18/oslte45"></imd-button>
      <imd-button context="main/frm18/clear"></imd-button>
    </div>
  </wr-content>
</wr-page>
